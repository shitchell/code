#!/usr/bin/env python3

import sys
import urllib
import requests

class StudentSearch:
	@url.getter
	def url():
		return "https://beartrack.ggc.edu/TracWeb40/ajxData.4sp?type=qpSearchStud&val="

	def search(name):
		name = urllib.parse.quote(name)
		response = requests.get(StudentSearch.url + name)
		data = response.read()
		return data

if __name__ == "__main__":
	for arg in sys.argv[1:]:
		data = StudentSearch.search(arg)
		print(data)
		